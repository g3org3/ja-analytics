---
import Layout from '~/layouts/Layout.astro'
---

<Layout title="ja-analytics">
  <div class="container mx-auto flex flex-col text-center pt-10 gap-3">
    <h1 class="text-3xl font-thin">ja-analytics</h1>
    <p class="font-thin">more coming soon.</p>
    <button
      class="ja-event-name=Click+Me bg-orange-500 hover:bg-orange-600 active:bg-orange-700 self-center text-white rounded-lg py-2 px-4"
      >click me</button
    >
  </div>
</Layout>
